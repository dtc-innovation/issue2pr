{
  "name": "issue-to-pr",
  "version": "1.0.0",
  "description": "Probot extension to convert an issue into a Pull Request.",
  "author": "dtc innovation <oncletom@users.noreply.github.com> (https://dtc-innovation.github.io/issue2pr)",
  "license": "ISC",
  "repository": "https://github.com/dtc-innovation/issue2pr.git",
  "homepage": "https://github.com/dtc-innovation/issue2pr",
  "bugs": "https://github.com/dtc-innovation/issue2pr/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^7.0.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
